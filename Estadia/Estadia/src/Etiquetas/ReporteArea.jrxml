	<queryString>
		<![CDATA[SELECT
     CONCAT(altabienes.nInventario,' ',altabienes.area,' ',altabienes.descripcion,' ',altabienes.marca_bien,' ',altabienes.modelo_bien,' ',altabienes.serie_bien) AS qr,
